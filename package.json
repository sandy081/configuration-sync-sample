{
	"name": "configuration-sync-sample",
	"displayName": "Configuration Sync Sample",
	"publisher": "sandy081",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.37.1"
	},
	"categories": [
		"Other"
	],
	"main": "./out/extension.js",
	"activationEvents": [
		"*"
	],
	"contributes": {
		"configuration": {
			"title": "FS Store",
			"properties": {
				"fsstore.sync.path": {
					"type": "string",
					"scope": "application",
					"description": "Path on disk to store user data"
				}
			}
		}
	},
	"enableProposedApi": true,
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.0-next.0",
		"@types/keytar": "^4.0.1"
	}
}